<!-- team -->
<div class="services">
    <div class="container">
        <h3 class="tittle_agile_w3">Amazing Team</h3>
        <div class="heading-underline">
            <div class="h-u1"></div><div class="h-u2"></div><div class="h-u3"></div><div class="clearfix"></div>
        </div>
        <div class="w3_services_grids">
            @foreach (var item in Model)
            {
                <div class="col-md-3 w3ls_team_grid">
                    <div class="w3ls_team_grid1 hover15">
                        <figure>
                            <img src="~/Uploads/team/@(item.ID).jpg" alt="@item.Name" class="img-responsive" />
                        </figure>
                        <div class="w3ls_team_grid1_pos">
                            <ul class="social-icons">
                                <li><a href="@item.Facebook" class="icon icon-border facebook"></a></li>
                                <li><a href="@item.Twitter" class="icon icon-border twitter"></a></li>
                                <li><a href="@item.Instgram" class="icon icon-border instagram"></a></li>
                            </ul>
                        </div>
                    </div>
                    <h4>@item.Name</h4>
                    <p>@item.Job</p>
                </div>
            }

            <div class="clearfix"> </div>
        </div>
    </div>
</div>
